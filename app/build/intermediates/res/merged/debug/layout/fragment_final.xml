<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kuba_10.frrelanceballparkestimator.Fragments.FinalFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="FINAL CALCULATION" />

        <TextView
            android:text="TextView work cost"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/workCost" />

        <TextView
            android:text="TextView calculated cost"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/costSum" />

        <TextView
            android:text="TextView estimation between"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/ballpark" />

        <Button
            android:text="Again"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/again" />
    </LinearLayout>

</FrameLayout>
